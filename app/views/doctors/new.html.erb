<% provide :title, t("create_doctor") %>
<div class="card card-primary center">
  <div class="card-header">
    <h3 class="card-title"><%= t "create_doctor" %></h3>
  </div>
  <form role="form">
    <div class="card-body">
      <div class="form-group">
        <label for="exampleInputEmail1"><%= t "email" %></label>
        <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email">
      </div>
      <div class="form-group">
        <label for="exampleInputPassword1"><%= t "password" %></label>
        <input type="password" class="form-control" id="password" placeholder="Password">
      </div>
      <div class="form-group">
        <label for="exampleInputPassword1"><%= t "password_confirmation" %></label>
        <input type="password" class="form-control" id="password_confirmation" placeholder="Password Confirmation">
      </div>
      <div class="form-group">
        <label for="room"><%= t "room" %></label>
        <input type="text" class="form-control" id="room" placeholder="Room">
      </div>
       <div class="form-group">
        <label for="position"><%= t "position" %></label>
        <input type="text" class="form-control" id="position" placeholder="Position">
      </div>
       <div class="form-group">
        <label for="acadamy"><%= t "acadamy" %></label>
        <input type="text" class="form-control" id="acadamy" placeholder="Acadamy">
      </div>
       <div class="form-group">
        <label for="experience"><%= t "experience" %></label>
        <input type="text" class="form-control" id="experience" placeholder="Experience">
      </div>
    </div>
    <div class="card-footer">
      <button type="submit" class="btn btn-primary"><%= t "submit" %></button>
    </div>
  </form>
</div>
